<h1>Reports by dates</h1>
<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Date</th>
      <th>Amount</th>
    </tr>
  </thead>
  <tbody>
    <% @operations.each do |operation| %>
      <tr>
        <td><%= operation[0] %></td>
        <td><%= operation[1] %></td>
      </tr>
    <% end %>
  </tbody>
</table>
